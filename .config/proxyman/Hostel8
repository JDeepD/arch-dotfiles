# Proxyman profile : Hostel8

# Example: 127.0.0.1
http_host=172.16.199.40

# Example: 8080
http_port=172.16.199.40

# Use same values for https and ftp
use_same=y

# use auth is 'y' or 'n', and provide username and password
use_auth=n
username=
password=

# If use same is 'y', no need of these
https_host=172.16.199.40
https_port=172.16.199.40
ftp_host=172.16.199.40
ftp_port=172.16.199.40

# URLs that should be excluded from proxying
no_proxy=nits.ac.in

