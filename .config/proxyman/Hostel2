# Proxyman profile : Hostel2

# Example: 127.0.0.1
http_host=172.16.30.20

# Example: 8080
http_port=8080

# Use same values for https and ftp
use_same=y

# use auth is 'y' or 'n', and provide username and password
use_auth=n
username=
password=

# If use same is 'y', no need of these
https_host=172.16.30.20
https_port=8080
ftp_host=172.16.30.20
ftp_port=8080

# URLs that should be excluded from proxying
no_proxy=nits.ac.in

